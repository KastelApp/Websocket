{
  "name": "ws",
  "description": "This is the Websocket for Kastel",
  "version": "0.0.5",
  "main": "src/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "tsupb": "tsup",
    "build": "turbo tsupb",
    "lint": "turbo run lint --parallel",
    "format": "turbo run format --parallel",
    "lint:affected:development": "turbo run lint --filter='...[origin/development]' --parallel",
    "format:affected:development": "turbo run format --filter='...[origin/development]' --parallel",
    "lint:affected:stable": "turbo run lint --filter='...[origin/stable]' --parallel",
    "format:affected:stable": "turbo run format --filter='...[origin/stable]' --parallel",
    "build:affected:development": "turbo run tsupb --filter='...[origin/development]' --parallel",
    "build:affected:stable": "turbo run tsupb --filter='...[origin/stable]' --parallel"
  },
  "keywords": [
    "chatting"
  ],
  "engines": {
    "node": "^18.0.0",
    "npm": "^8.0.0"
  },
  "author": "DarkerInk",
  "license": "GPL-3.0-or-later",
  "types": "index.d.ts",
  "repository": "https://github.com/Kastelll/ws",
  "packageManager": "yarn@3.3.0",
  "dependencies": {
    "@kastelll/core": "^0.2.2",
    "@kastelll/util": "^0.1.3",
    "@sentry/node": "^7.49.0",
    "chalk": "^5.2.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.5",
    "redis": "^4.6.5",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.1",
    "@commitlint/config-angular": "^17.6.1",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/node": "^18.16.0",
    "@types/ws": "^8.5.4",
    "eslint": "^8.39.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1"
  }
}
