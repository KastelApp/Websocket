{
	"name": "websocket",
	"description": "The websocket server for Kastelll",
	"version": "1.0.0",
	"main": "dist/index.js",
	"dependencies": {
		"@kastelll/cassandra-driver": "^4.6.15",
		"@kastelll/util": "0.2.2",
		"@sentry/node": "^7.70.0",
		"archiver": "^5.3.1",
		"copyfiles": "^2.4.1",
		"express": "^4.18.2",
		"jsonwebtoken": "^9.0.1",
		"simple-git": "^3.19.1",
		"ws": "^8.13.0"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.6.7",
		"@commitlint/config-angular": "^17.6.7",
		"@types/archiver": "^5.3.2",
		"@types/bcrypt": "^5.0.0",
		"@types/express": "^4.17.17",
		"@types/jsonwebtoken": "^9.0.2",
		"@types/node": "^20.4.8",
		"@types/ws": "^8.5.5",
		"cross-env": "^7.0.3",
		"eslint": "^8.46.0",
		"eslint-config-neon": "^0.1.47",
		"eslint-formatter-pretty": "^5.0.0",
		"lint-staged": "^13.2.3",
		"prettier": "^3.0.1",
		"tsup": "^7.2.0"
	},
	"scripts": {
		"start": "node dist/index.js",
		"build": "tsup && yarn postbuild",
		"lint": "prettier --check . && cross-env TIMING=1 eslint src --ext .js,.ts --format=pretty",
		"format": "prettier --write . && cross-env TIMING=1 eslint src --ext .js,.ts --fix --format=pretty",
		"dev": "yarn build && node dist/index.js --dev",
		"postbuild": "copyfiles -u 1 \"src/Utils/Cql/Tables/*\" dist"
	},
	"keywords": [
		"chatting"
	],
	"engines": {
		"node": "^18.0.0",
		"npm": "^8.0.0"
	},
	"contributors": [
		{
			"email": "darkerinker@gmail.com",
			"name": "DarkerInk"
		}
	],
	"license": "GPL-3.0-or-later",
	"repository": {
		"type": "git",
		"url": "https://github.com/Kastelll/Websocket.git"
	},
	"packageManager": "yarn@3.3.0",
	"type": "module"
}
